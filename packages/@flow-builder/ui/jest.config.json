{
    "preset": "@lwc/jest-preset",
    "verbose": true,
    "transformIgnorePatterns": ["/node_modules/(?!lightning-components-stubs/modules/lightning-stubs/)"],
    "testPathIgnorePatterns": ["/build/"],
    "moduleNameMapper": {
        "^aura$": "<rootDir>/jest-modules/aura/aura",
        "^(aura-instrumentation|instrumentation/service|o11y/client)$": "<rootDir>/jest-modules/aura/auraInstrumentation",
        "^aura-storage$": "<rootDir>/jest-modules/aura/auraStorage",
        "^mock/(flows|debugResponse)/(.+)$": "<rootDir>/jest-mock-data/$1/$2",
        "^mock/(.+)$": "<rootDir>/jest-mock-data/$1/$1",

        "^builder_platform_interaction/sharedUtils/(commands|lwcUtils|auraUtils|commonUtils|keyboardInteractionUtils)": "<rootDir>/../shared-utils/src/$1",
        "^builder_platform_interaction/builderTestUtils/(commonTestUtils)$": "<rootDir>/../ui/src/builder_platform_interaction/builderTestUtils/$1",
        "^builder_platform_interaction_mocks/sharedUtils/(keyboardInteractionUtils)$": "<rootDir>/../shared-utils/jest-modules/builder_platform_interaction/sharedUtils/$1/$1",
        "^builder_platform_interaction_mocks/sharedUtils/(sharedUtils)$": "<rootDir>/../shared-utils/jest-modules/builder_platform_interaction/$1/$1",

        "^(builder_platform_interaction)/alcMenuTemplate$": "<rootDir>/../auto-layout-canvas-ui/src/builder_platform_interaction/alcMenuTemplate/alcMenuTemplate",
        "^(builder_platform_interaction)/alcMenu$": "<rootDir>/../auto-layout-canvas-ui/src/builder_platform_interaction/alcMenu/alcMenu",
        "^(builder_platform_interaction)/alcConnectorMenu$": "<rootDir>/../auto-layout-canvas-ui/src/builder_platform_interaction/alcConnectorMenu/alcConnectorMenu",
        "^(builder_platform_interaction)/alcNodeMenu$": "<rootDir>/../auto-layout-canvas-ui/src/builder_platform_interaction/alcNodeMenu/alcNodeMenu",
        "^(builder_platform_interaction)/zoomPanel$": "<rootDir>/../auto-layout-canvas-ui/src/builder_platform_interaction/zoomPanel/zoomPanel",

        "^builder_platform_interaction/sharedUtils/commands": "<rootDir>/../shared-utils/src/commands",
        "^builder_platform_interaction/sharedUtils/lwcUtils": "<rootDir>/../shared-utils/src/lwcUtils",
        "^builder_platform_interaction/sharedUtils/auraUtils": "<rootDir>/../shared-utils/src/auraUtils",
        "^builder_platform_interaction/sharedUtils/commonUtils": "<rootDir>/../shared-utils/src/commonUtils",
        "^builder_platform_interaction/sharedUtils/customIconUtils": "<rootDir>/../shared-utils/src/customIconUtils",

        "^(builder_platform_interaction)/richHelpTooltip": "<rootDir>/../ui/src/builder_platform_interaction/fieldInput/richHelpTooltip/richHelpTooltip",

        "^(builder_platform_interaction)/alcStartMenu$": "<rootDir>/../ui/src/builder_platform_interaction/alcStartMenu/alcStartMenu",
        "^(builder_platform_interaction)/builderTestUtils/commonTestUtils$": "<rootDir>/../ui/src/builder_platform_interaction/builderTestUtils/commonTestUtils",
        "^(builder_platform_interaction)/builderTestUtils/domTestUtils$": "<rootDir>/../ui/src/builder_platform_interaction/builderTestUtils/domTestUtils",
        "^builder_platform_interaction/sharedUtils/keyboardInteractionUtils": "<rootDir>/../shared-utils/src/keyboardInteractionUtils",

        "^builder_platform_interaction_mocks/sharedUtils/sharedUtils$": "<rootDir>/../shared-utils/jest-modules/builder_platform_interaction/sharedUtils/sharedUtils",
        "^builder_platform_interaction_mocks/sharedUtils$": "<rootDir>/../auto-layout-canvas-ui/jest-modules/builder_platform_interaction/sharedUtils/sharedUtils",
        "^builder_platform_interaction_mocks/(.+)$": "<rootDir>/jest-modules/builder_platform_interaction/$1/$1",
        "^serverData/(.+)/(.+)$": "<rootDir>/jest-mock-data/results/FlowBuilderControllerGoldFileTest/test$1Serialization/$2",
        "^builder_platform_interaction/autoLayoutCanvas$": "<rootDir>/../auto-layout-canvas/dist/index.dev.js",
        "^builder_platform_interaction/sharedUtils$": "<rootDir>/../shared-utils/dist/index.esNext.js",
        "^builder_platform_interaction/(alcMenuTemplate|alcConnectorMenu|alcNodeMenu|zoomPanel|alcCanvas|alcEvents|alcMenu|alcComponentsUtils|menu|popover|contextualMenuUtils)$": "<rootDir>/../auto-layout-canvas-ui/src/builder_platform_interaction/$1/$1",
        "^(builder_platform_interaction)/(.+)$": "<rootDir>/src/$1/$2/$2",
        "^(builder_framework)/(.+)$": "<rootDir>/../shared-utils/jest-modules/$1/$2/$2",
        "^lightning/(configProvider|input|textarea|groupedCombobox|baseCombobox|baseComboboxItem|inputRichText|popup|primitiveIcon|platformShowToastEvent)$": "<rootDir>/jest-modules/lightning/$1/$1",
        "lightning/(inputUtils|utilsPrivate)": "<rootDir>/../../../node_modules/lwc-components-lightning/src/lightning/$1/$1",
        "^lightning/(.+)$": "<rootDir>/../../../node_modules/lightning-components-stubs/modules/lightning-stubs/core/$1/$1",
        "^analyzer_framework/(.+)$": "<rootDir>/jest-modules/analyzer_framework/$1/$1",
        "^formula/(.+)$": "<rootDir>/jest-modules/formula/$1/$1",
        "^(force)/(.+)$": "<rootDir>/jest-modules/$1/$2/$2",
        "@salesforce/apex/interaction.FlowBuilderController.getTimeZones": "<rootDir>/jest-modules/apex/interaction/getTimeZones.js"
    },
    "setupFilesAfterEnv": ["<rootDir>/jestSetup.js"],
    "clearMocks": true,
    "coverageDirectory": "../../../coverage",
    "collectCoverageFrom": [
        "<rootDir>/src/*/!(builderTestUtils)**/*",
        "!**/*Labels*",
        "!**/integrationTests/**/*",
        "!**/__tests__/**/*"
    ]
}
